{
  "name": "@ts-monorepo/local-lambda-server",
  "version": "2.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "sleep 5 && ts-node-dev -r tsconfig-paths/register src/index.ts --rs --watch ../../.env --respawn src/index.ts ",
    "force:stop": "echo forcestop $ kill $(lsof -i:3100 | grep node | cut -d' ' -f2- )",
    "start:mongo:db": "docker-compose up > /dev/null",
    "start:mongo:db:inline": "docker-compose up -d > /dev/null && yarn wait-port 27016",
    "start:mongo:ui": "mongo-gui -u mongodb://localhost:27016/"
  },
  "dependencies": {
    "@ts-monorepo/api": "0.0.1",
    "@types/node": "^20.4.8",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "jwk-to-pem": "^2.0.5",
    "lambda-local": "^2.1.1",
    "lodash": "^4.17.21",
    "mongo-gui": "^0.0.18",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.21",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "if-env": "^1.0.4",
    "wait-port": "^1.0.4"
  }
}
